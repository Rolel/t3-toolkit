{
  "name": "smile/t3_toolkit",
  "type": "typo3-cms-extension",
  "description": "Tools for TYPO3 website",
  "homepage": "https://github.com/Rolel/t3-toolkit",
  "license": ["GPL-2.0"],
  "authors": [
    { "name": "Rolel", "email": "typo3@smile.fr" }
  ],
  "keywords": [
    "typo3",
    "typoscript",
    "toolkit",
    "smile"
  ],
  "repositories": [
    { "type": "composer", "url": "https://composer.typo3.org/" }
  ],
  "require": {
    "php": "5.5.0 - 7.0.99",
    "typo3/cms-core": ">=7.6",
  },
  "require-dev": {
  },
  "replace": {
    "smile_t3_toolkit": "self.version",
    "smile_t3_base": "self.version",
    "typo3-ter/smile-t3-toolkit": "self.version",
    "typo3-ter/smile-t3-base": "self.version",
    "smile_base": "self.version"
  }
}
